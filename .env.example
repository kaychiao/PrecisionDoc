# PrecisionDoc Environment Variables Example
# Copy this file to .env and fill in your values

# API Key (required)
# Your API key for OpenAI or other LLM provider
API_KEY=your_api_key_here

# Base URL (optional)
# Base URL for API endpoint, defaults to OpenAI if not specified
BASE_URL=https://api.openai.com/v1

# Model name (optional)
# The model to use for text processing
OPENAI_MODEL=gpt-4

# Logging level (optional)
# Set to DEBUG, INFO, WARNING, ERROR, or CRITICAL
LOG_LEVEL=INFO
